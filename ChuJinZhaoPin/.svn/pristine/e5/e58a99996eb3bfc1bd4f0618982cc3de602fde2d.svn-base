-(UIView *)tableView:(UITableView *)tableView viewForHeaderInSection:(NSInteger)section{
    UIView *view = [[UIView alloc]initWithFrame:CGRectMake(0, 0, kScreenWidth, Size(44))];
    
    ResumeHeaderView *headV = [[ResumeHeaderView alloc]init];
    headV.frame = CGRectMake(Size(15), 0, kScreenWidth-Size(30), Size(44));
    UIBezierPath *maskPath = [UIBezierPath bezierPathWithRoundedRect:headV.bounds byRoundingCorners:(UIRectCornerTopLeft | UIRectCornerTopRight) cornerRadii:CGSizeMake(Size(5),Size(5))];//圆角大小
    CAShapeLayer *maskLayer = [[CAShapeLayer alloc] init];
    maskLayer.frame = headV.bounds;
    maskLayer.path = maskPath.CGPath;
    headV.layer.mask = maskLayer;
    [view addSubview:headV];
    
    
    [headV.addBtn setImage:[UIImage imageNamed:@"职位详情++"] forState:(UIControlState)UIControlStateNormal];
    
    
    switch (section) {
        case 0:{
            
            headV.addBtn.hidden = YES;
            headV.titleLab.text = @"求职意向";
            
        }
            break;
        case 1:{
            if (self.resumeInitM.data.tCjResumeDto.resumeWorkExperienceList.count == 0) {
                headV.addBtn.hidden = YES;
            }else{
                headV.addBtn.hidden = NO;
            }
            headV.titleLab.text = @"工作经验";
            headV.addBtnBlock = ^{
                JobExperienceViewController *vc = [JobExperienceViewController new];
                vc.resumeId = self.resumeId;
                [self.navigationController pushViewController:vc animated:YES];
            };
        }
            break;
        case 2:{
            if (self.resumeInitM.data.tCjResumeDto.resumeProjectExperienceList.count == 0) {
                headV.addBtn.hidden = YES;
            }else{
                headV.addBtn.hidden = NO;
            }
            headV.titleLab.text = @"项目经验";
            headV.addBtnBlock = ^{
                ProjectExperienceViewController *vc = [ProjectExperienceViewController new];
                vc.resumeId = self.resumeId;
                [self.navigationController pushViewController:vc animated:YES];
            };
        }
            break;
        case 3:{
            if (self.resumeInitM.data.tCjResumeDto.resumeEduExperienceList.count == 0) {
                headV.addBtn.hidden = YES;
            }else{
                headV.addBtn.hidden = NO;
            }
            headV.titleLab.text = @"教育经历";
            headV.addBtnBlock = ^{
                EducationExperienceViewController *vc = [EducationExperienceViewController new];
                vc.resumeId = self.resumeId;
                [self.navigationController pushViewController:vc animated:YES];
            };
        }
            break;
        case 4:{
            if (self.resumeInitM.data.tCjResumeDto.resumeSkillPointList.count == 0) {
                headV.addBtn.hidden = YES;
            }else{
                headV.addBtn.hidden = NO;
            }
            headV.titleLab.text = @"技能证书";
            [headV.addBtn setImage:[UIImage imageNamed:@"编辑"] forState:(UIControlState)UIControlStateNormal];
            headV.addBtnBlock = ^{
                SkillViewController *vc = [SkillViewController new];
                
                if (self.resumeInitM.data.tCjResumeDto.resumeSkillPointList.count > 0) {
                    vc.skillId = NSStringFromInteger(self.resumeInitM.data.tCjResumeDto.resumeSkillPointList.firstObject.ID);
                    vc.resumeId = NSStringFromInteger(self.resumeInitM.data.tCjResumeDto.resumeSkillPointList.firstObject.resumeId);;
                }
                
                [self.navigationController pushViewController:vc animated:YES];
            };
        }
            break;
        default:
            break;
    }
    
    
    return view;
}
