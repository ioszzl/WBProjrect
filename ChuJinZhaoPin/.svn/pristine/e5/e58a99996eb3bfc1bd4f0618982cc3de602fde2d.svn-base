#pragma mark - 懒加载
-(ResumeMineInfoView *)mineInfoV{
    if (!_mineInfoV) {
        _mineInfoV = [[ResumeMineInfoView alloc]init];
        _mineInfoV.layer.cornerRadius = Size(5);
        [self.tableV.tableHeaderView addSubview:_mineInfoV];
        [_mineInfoV mas_makeConstraints:^(MASConstraintMaker *make) {
            make.left.top.mas_offset(Size(15));
            make.right.mas_offset(-Size(15));
            make.bottom.mas_offset(-Size(10));
        }];
        
        WEAKSELF;
        _mineInfoV.editInfoBlock = ^{
            ResumeMineInfoViewController *vc = [ResumeMineInfoViewController new];
            vc.ID = NSStringFromInteger(weakSelf.resumeInitM.data.tCjResumeDto.tCjUserLoginDto.tCjUserInfoDto.ID);
            vc.resumeId = weakSelf.resumeId;
            [weakSelf.navigationController pushViewController:vc animated:YES];
        };
    }
    return _mineInfoV;
}
-(AddResumeView *)addInfoV{
    if (!_addInfoV) {
        _addInfoV = [[AddResumeView alloc] init];
        _addInfoV.layer.cornerRadius = Size(5);
        [self.tableV.tableHeaderView addSubview:_addInfoV];
        [_addInfoV mas_makeConstraints:^(MASConstraintMaker *make) {
            make.left.top.mas_offset(Size(15));
            make.right.mas_offset(-Size(15));
            make.bottom.mas_offset(-Size(10));
        }];
        
        WEAKSELF;
        _addInfoV.editInfoBlock = ^{
            ResumeMineInfoViewController *vc = [ResumeMineInfoViewController new];
            [weakSelf.navigationController pushViewController:vc animated:YES];
        };
    }
    return _addInfoV;
}
