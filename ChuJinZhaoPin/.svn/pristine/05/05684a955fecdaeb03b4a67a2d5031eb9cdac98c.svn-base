@implementation ChangeRoleViewController

- (void)viewDidLoad {
    [super viewDidLoad];
    
    [self initUI];
    // Do any additional setup after loading the view.
}
-(void)initUI{
    self.title = @"切换身份";
    self.view.backgroundColor = RGBCOLOR(248, 249, 251);
    UIImageView *imgV = [[UIImageView alloc]initWithImage:[UIImage imageNamed:@"切换身份"]];
    [self.view addSubview:imgV];
    [imgV mas_makeConstraints:^(MASConstraintMaker *make) {
        make.width.height.mas_equalTo(Size(370/2));
        make.top.mas_offset(Size(154/2));
        make.centerX.equalTo(self.view);
    }];
    
    UIButton *changeBtn = [UIButton ym_ButtonWithFrame:CGRectZero title:@"我要招聘" backgroundColor:COLOR_MAIN_PERSON titleColor:COLOR_RGB_51 titleSize:Size(17)];
    changeBtn.layer.cornerRadius = Size(68/4);
    [self.view addSubview:changeBtn];
    [changeBtn mas_makeConstraints:^(MASConstraintMaker *make) {
        make.width.mas_equalTo(Size(260/2));
        make.height.mas_equalTo(Size(68/2));
        make.top.equalTo(imgV.mas_bottom).mas_offset(Size(72/2));
        make.centerX.equalTo(imgV);
    }];
    [changeBtn addTarget:self action:@selector(doChangeBtn) forControlEvents:UIControlEventTouchUpInside];
    
}


-(void)doChangeBtn{
    
