#import "BaseModel.h"

NS_ASSUME_NONNULL_BEGIN

@interface ResumeList :NSObject
@property (nonatomic , assign) NSInteger              post_id;//岗位id
@property (nonatomic , assign) NSInteger              status;//主状态  0.已投递 1.已查看 2.已邀请 3.求职者接受邀请  4.结束状态(到达面试时间或其它终止条件)
@property (nonatomic , assign) NSInteger              create_time;//投递时间
@property (nonatomic , copy) NSString              * group_name;
@property (nonatomic , copy) NSString              * resume_type;
@property (nonatomic , copy) NSString              * company_address;//公司地址
@property (nonatomic , assign) NSInteger              resume_id;//简历id,
@property (nonatomic , assign) NSInteger              max_edu_level;//学历,1:大专,2:本科,3:研究生,4:博士
@property (nonatomic , assign) NSInteger              interview_user_id;//面试人id
@property (nonatomic , copy) NSString              * position_name;//投递的岗位就职区域
@property (nonatomic , copy) NSString              * host_name;
@property (nonatomic , assign) NSInteger              face_time;//面试时间
@property (nonatomic , copy) NSString              * fresh_graduates;//是否应届生(0否,1是)
@property (nonatomic , copy) NSString              * work_position;//岗位
@property (nonatomic , copy) NSString              * describ;
@property (nonatomic , copy) NSString              * metting_sequence;//面试编号
@property (nonatomic , copy) NSString              * want_salary;//期望薪资
@property (nonatomic , copy) NSString              * ceil_phone;//电话
@property (nonatomic , assign) NSInteger              work_year;//工作年限(单位:年)
@property (nonatomic , assign) NSInteger              work_address_region_id;//投递的岗位就职id
@property (nonatomic , assign) NSInteger              cancel_status;//结束状态 0.简历不合适（企业） 1.企业邀请后求职者拒绝   2.接受邀请后求职者主动取消（面试前2个小时） 3.接受邀请后企业取消 4.到达面试时间自动结束 5.面试成功（企业反馈） 6.面试失败 （企业反馈）
@property (nonatomic , copy) NSString              * imgUrl;//求职者头像
@property (nonatomic , copy) NSString              * phone;
@property (nonatomic , assign) NSInteger              metting_id;//面试id
@property (nonatomic , copy) NSString              * skill;//技能,
@property (nonatomic , copy) NSString              * end_time;
@property (nonatomic , copy) NSString              * fpath;
@property (nonatomic , copy) NSString              * user_cancel_reason;
@property (nonatomic , copy) NSString              * real_name;//面试人真实姓名
@property (nonatomic , copy) NSString              * job_name;//面试的岗位名称
@property (nonatomic, assign) NSInteger             age;
@property (nonatomic, copy) NSString               * sex;

@end

@interface ResumeListData :NSObject
@property (nonatomic , assign) NSInteger              pageSize;
@property (nonatomic , assign) BOOL              hasNextPage;
@property (nonatomic , assign) NSInteger              pages;
@property (nonatomic , assign) NSInteger              pageNum;
@property (nonatomic , assign) NSInteger              lastPage;
@property (nonatomic , copy) NSArray<NSNumber *>              * navigatepageNums;
@property (nonatomic , assign) BOOL              isLastPage;
@property (nonatomic , assign) NSInteger              total;
@property (nonatomic , copy) NSString              * orderBy;
@property (nonatomic , assign) NSInteger              nextPage;
@property (nonatomic , assign) NSInteger              size;
@property (nonatomic , assign) BOOL              hasPreviousPage;
@property (nonatomic , assign) NSInteger              navigatePages;
@property (nonatomic , assign) NSInteger              startRow;
@property (nonatomic , assign) NSInteger              prePage;
@property (nonatomic , copy) NSArray<ResumeList *>              * list;
@property (nonatomic , assign) NSInteger              firstPage;
@property (nonatomic , assign) BOOL              isFirstPage;
@property (nonatomic , assign) NSInteger              endRow;

@end


@interface ResumeListModel : BaseModel
@property (nonatomic , strong) ResumeListData              * data;
@end

NS_ASSUME_NONNULL_END
